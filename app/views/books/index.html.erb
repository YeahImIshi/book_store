<h1>All Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Height</th>
      <th>Publisher</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= link_to book.title, book_path(book) %></td>
        <td><%= book.author %></td>
        <td><%= book.height %></td>
        <td><%= book.publisher %></td>
        <td><%= book.genre.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @books %>





